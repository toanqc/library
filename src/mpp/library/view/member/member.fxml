<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" stylesheets="@css/member.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mpp.library.controller.MemberController">
   <children>
      <GridPane hgap="15.0" prefHeight="104.0" prefWidth="530.0" vgap="15.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Button fx:id="btnHome" mnemonicParsing="false" onMouseClicked="#returnHome" text="Home" GridPane.halignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#saveMember" text="Save" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
            <Label fx:id="lblStatus" text="All fields are mandatory" GridPane.columnSpan="3" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <GridPane hgap="15.0" layoutY="100.0" prefHeight="255.0" prefWidth="480.0" vgap="15.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="400.0" minWidth="10.0" prefWidth="399.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="401.0" minWidth="10.0" prefWidth="401.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="401.0" minWidth="10.0" prefWidth="401.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="401.0" minWidth="10.0" prefWidth="401.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Label text="First Name" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
            <Label text="Last Name" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
            <Label text="Street" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
            <Label text="City" GridPane.halignment="RIGHT" GridPane.rowIndex="4" />
            <Label text="State" GridPane.halignment="RIGHT" GridPane.rowIndex="5" />
            <Label text="Zip" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="5" />
            <Label text="Phone" GridPane.halignment="RIGHT" GridPane.rowIndex="6" />
            <TextField fx:id="txtFirstName" onKeyReleased="#onFirstNameChanged" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="1" />
            <TextField fx:id="txtLastName" onKeyReleased="#onLastNameChanged" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="2" />
            <TextField fx:id="txtCity" onKeyReleased="#onCityChanged" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="4" />
            <TextField fx:id="txtStreet" onKeyReleased="#onStreetChanged" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="3" />
            <TextField fx:id="txtState" onKeyReleased="#onStateChanged" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="txtZip" onKeyReleased="#onZipChanged" GridPane.columnIndex="3" GridPane.rowIndex="5" />
            <TextField fx:id="txtPhone" onKeyReleased="#onPhoneChanged" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="6" />
            <Label text="Member ID" GridPane.halignment="RIGHT" />
            <TextField fx:id="txtMemberId" editable="false" styleClass="disable" GridPane.columnIndex="1" GridPane.columnSpan="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
