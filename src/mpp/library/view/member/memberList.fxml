<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mpp.library.controller.MemberListController">
   <children>
      <GridPane hgap="15.0" vgap="15.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="600.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="0.0" prefHeight="490.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Button fx:id="btnHome" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#returnHome" prefHeight="26.0" prefWidth="75.0" styleClass="lion" stylesheets="@../css/common.css" text="Home" GridPane.halignment="LEFT">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icon/home.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Label fx:id="lblStatus" styleClass="title" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
            <Button fx:id="btnAdd" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#addMember" prefHeight="26.0" prefWidth="75.0" styleClass="lion" text="Add" GridPane.columnIndex="2" GridPane.halignment="RIGHT">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icon/add.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <TextField fx:id="txtSearch" GridPane.columnSpan="2" GridPane.rowIndex="1" />
            <Button fx:id="btnSearch" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#searchMember" prefHeight="26.0" prefWidth="75.0" styleClass="lion" text="Find" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
               <graphic>
                  <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icon/search.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="3" GridPane.rowIndex="2">
               <content>
                  <TableView fx:id="memberTable" prefHeight="485.0" prefWidth="780.0">
                    <columns>
                      <TableColumn fx:id="memberId" maxWidth="70.0" minWidth="70.0" prefWidth="70.0" styleClass="column-link" text="ID" />
                      <TableColumn fx:id="firstName" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" text="First Name" />
                        <TableColumn fx:id="lastName" maxWidth="90.0" minWidth="90.0" prefWidth="90.0" text="Last Name" />
                        <TableColumn fx:id="street" maxWidth="120.0" minWidth="120.0" prefWidth="120.0" sortable="false" text="Street" />
                        <TableColumn fx:id="city" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="City" />
                        <TableColumn fx:id="state" maxWidth="60.0" minWidth="60.0" prefWidth="60.0" text="State" />
                        <TableColumn fx:id="zip" maxWidth="65.0" minWidth="65.0" prefWidth="65.0" sortable="false" text="Zip" />
                        <TableColumn fx:id="phone" maxWidth="105.0" minWidth="105.0" prefWidth="105.0" sortable="false" text="Phone" />
                        <TableColumn fx:id="checkoutRecord" maxWidth="78.0" minWidth="78.0" prefWidth="78.0" sortable="false" styleClass="column-link" text="CR" />
                    </columns>
                  </TableView>
               </content>
            </ScrollPane>
         </children>
      </GridPane>
   </children>
   <stylesheets>
      <URL value="@../css/common.css" />
   </stylesheets>
</AnchorPane>
