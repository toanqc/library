<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane stylesheets="@../css/common.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mpp.library.controller.MemberListController">
   <children>
      <GridPane hgap="15.0" prefHeight="600.0" prefWidth="800.0" vgap="15.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="635.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="0.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="0.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="520.0" minHeight="0.0" prefHeight="493.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Button fx:id="btnHome" alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#returnHome" prefWidth="120.0" styleClass="lion" stylesheets="@../css/common.css" text="Home" GridPane.halignment="LEFT">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin></Button>
            <Label styleClass="title" text="All Library Member" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
            <Button fx:id="btnAdd" alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#addMember" prefWidth="110.0" styleClass="lion" text="Add" GridPane.columnIndex="2" GridPane.halignment="RIGHT" />
            <TextField fx:id="txtSearch" GridPane.columnSpan="2" GridPane.rowIndex="1" />
            <Button fx:id="btnSearch" alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#searchMember" prefWidth="110.0" styleClass="lion" text="Find" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
            <TableView fx:id="memberTable" GridPane.columnSpan="3" GridPane.rowIndex="2">
              <columns>
                <TableColumn fx:id="memberId" prefWidth="90.0" text="Member ID" />
                <TableColumn fx:id="firstName" prefWidth="94.0" text="First Name" />
                  <TableColumn fx:id="lastName" prefWidth="89.0" text="Last Name" />
                  <TableColumn fx:id="street" prefWidth="119.0" sortable="false" text="Street" />
                  <TableColumn fx:id="city" prefWidth="62.0" text="City" />
                  <TableColumn fx:id="state" prefWidth="46.0" text="State" />
                  <TableColumn fx:id="zip" prefWidth="63.0" sortable="false" text="Zip" />
                  <TableColumn fx:id="phone" minWidth="0.0" prefWidth="85.0" sortable="false" text="Phone" />
                  <TableColumn fx:id="checkoutRecord" maxWidth="130.0" prefWidth="130.0" sortable="false" text="Checkout Record" />
              </columns>
            </TableView>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icon/home.png" />
               </image>
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </ImageView>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2">
               <image>
                  <Image url="@../icon/add.png" />
               </image>
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </ImageView>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="1">
               <image>
                  <Image url="@../icon/search.png" />
               </image>
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </ImageView>
         </children>
      </GridPane>
   </children>
</AnchorPane>
