<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" stylesheets="@../css/common.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mpp.library.controller.PublicationOverdueController">
  <!-- TODO Add Nodes -->
  <children>
      <GridPane hgap="15.0" prefHeight="600.0" prefWidth="800.0" vgap="15.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="0.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="565.0" minHeight="10.0" prefHeight="535.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
        <Button alignment="CENTER_RIGHT" blendMode="SRC_OVER" mnemonicParsing="false" onMouseClicked="#returnHome" prefHeight="25.0" prefWidth="85.0" text="Home">
          <font>
            <Font name="System Bold" size="12.0" fx:id="x1" />
          </font>
        </Button>
          <ScrollPane GridPane.rowIndex="1">
               <content>
              <TableView fx:id="tableView">
                <columns>
                  <TableColumn fx:id="isbnColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="165.0" text="ISBN/Issue No" />
                  <TableColumn fx:id="typeColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="119.0" text="Type" />
                  <TableColumn fx:id="checkoutDateColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="156.0" text="Checkout Date" />
                  <TableColumn fx:id="duedateColumn" maxWidth="5000.0" minWidth="10.0" prefWidth="154.0" text="Due Date" />
                        <TableColumn fx:id="memberColumn" minWidth="0.0" prefWidth="173.0" text="Member" />
                </columns>
              </TableView>
               </content>
          </ScrollPane>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icon/home.png" />
               </image>
               <GridPane.margin>
                  <Insets left="15.0" />
               </GridPane.margin>
            </ImageView>
         </children>
      </GridPane>

  </children>
</AnchorPane>
