<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="509.0" prefWidth="626.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="mpp.library.controller.PrintCheckoutRecordController">
  <!-- TODO Add Nodes -->
  <children>
    <FlowPane alignment="CENTER" hgap="60.0" layoutX="0.0" layoutY="0.0" minHeight="43.0" prefHeight="43.0" prefWidth="612.0" vgap="5.0">
      <children>
        <Button fx:id="btnHome" blendMode="SRC_OVER" mnemonicParsing="false" onMouseClicked="#gotoMainScreen" prefHeight="30.0" prefWidth="192.0" text="Home">
          <font>
            <Font name="System Bold" size="12.0" fx:id="x1" />
          </font>
          <FlowPane.margin>
            <Insets />
          </FlowPane.margin>
        </Button>
        <Button fx:id="btnCheckout" defaultButton="true" font="$x1" mnemonicParsing="false" onMouseClicked="#printCheckoutRecord" prefHeight="30.0" prefWidth="241.0" text="Print Checkout Record" />
      </children>
      <padding>
        <Insets bottom="10.0" left="15.0" top="20.0" />
      </padding>
    </FlowPane>
    <GridPane hgap="20.0" layoutX="14.0" layoutY="40.0" prefWidth="598.0" vgap="0.0">
      <children>
        <TextField fx:id="txtMemberID" onAction="#onEnter" prefHeight="30.0" prefWidth="579.0" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <Button fx:id="btnSearch" blendMode="SRC_OVER" font="$x1" mnemonicParsing="false" onMouseClicked="#search" prefHeight="30.0" prefWidth="165.0" text="Search" GridPane.columnIndex="1" GridPane.rowIndex="0" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="394.0" minWidth="10.0" prefWidth="368.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="234.0" minWidth="10.0" prefWidth="104.0" />
      </columnConstraints>
      <padding>
        <Insets top="10.0" />
      </padding>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
    <ScrollPane layoutX="14.0" layoutY="125.0" prefHeight="356.0" prefWidth="579.0">
      <content>
        <TableView fx:id="tableView" prefHeight="318.0" prefWidth="564.0">
          <columns>
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="98.0" text="ISBN/Issue No" fx:id="ISBNTC" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="147.0" text="Title" fx:id="titleTC" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="65.0" text="Type" fx:id="typeTC" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="150.0" text="Checkout Date" fx:id="chkoutDateTC" />
            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="76.0" text="Due Date" fx:id="dueDateTC" />
          </columns>
        </TableView>
      </content>
    </ScrollPane>
    <Label fx:id="lblMessage" alignment="CENTER" labelFor="$txtMemberID" layoutX="15.0" layoutY="86.0" prefHeight="30.0" prefWidth="579.0" text="" textFill="RED" visible="false">
      <font>
        <Font name="System Bold" size="14.0" />
      </font>
    </Label>
  </children>
</AnchorPane>
